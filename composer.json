{
    "name": "reesmcivor/laravel-secure-login",
    "description": "Laravel Secure Login",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Rees McIvor",
            "email": "reesmcivor@hotmail.com"
        }
    ],
    "require": {
        "php": "^8.1"
    },
    "autoload": {
        "psr-4": {
            "ReesMcIvor\\SecureLogin\\": "src/",
            "ReesMcIvor\\SecureLogin\\Database\\Factories\\": "database/factories/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "ReesMcIvor\\SecureLogin\\SecureLoginPackageServiceProvider"
            ]
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "version": "0.0.1"
}
